{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 5 versions", "not ie <= 8"]
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-typescript", {
      "allowDeclareFields": true
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ]
}